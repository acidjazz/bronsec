
@keyframes fadeIn
  from
    opacity 0
  to
    opacity 1

@keyframes cellInY
  from
    transform translate(0, 10px)
    opacity 0
  to
    transform translate(0, 0)
    opacity 1

@keyframes cellInX
  from
    transform translate(10px, 0)
    opacity 0
  to
    transform translate(0, 0)
    opacity 1

@keyframes cellOutY
  from
    transform translate(0, -10px)
    opacity 0
  to
    transform translate(0, 0)
    opacity 1

@keyframes filters
  from
    transform translate(0, -5px)
    opacity 0.5
  to
    transform translate(0, 0)
    opacity 1

@keyframes cellOutX
  from
    transform translate(-10px, 0)
    opacity 0
  to
    transform translate(0, 0)
    opacity 1

@keyframes slowzoom
  from
    background-size 150%
  to
    background-size 250%

