.header
  border-bottom: 1px solid c.grey1
  padding 0px 20px
  margin 0 0 10px 0
  > .logo
    float left
    width 50px
    height 50px
    padding 10px 0
    > svg
      width inherit
      height inherit
  > .menu
    float left
    margin 20px 0 0 20px
    > .item
      cursor pointer
      display inline-block
      padding 10px 20px
      &:hover
        text-decoration underlin
      &.active
        font-weight bold
